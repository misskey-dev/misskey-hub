{
	summary: 'ノートを検索します。',
	description: 'ノートを検索します。',
	tags: [
		'notes',
	],
	requireCredential: false,
	req: {
		type: 'object',
		properties: {
			query: {
				type: 'string',
				description: '検索クエリ。',
			},
			sinceId: {
				type: 'string',
				format: 'misskey:id',
			},
			untilId: {
				type: 'string',
				format: 'misskey:id',
			},
			limit: {
				type: 'integer',
